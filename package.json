{
	"name": "idealab-code-challenge",
	"version": "1.0.0",
	"description": "Idealab code challenge",
	"main": "app.js",
	"repository": "https://github.com/eokwukwe/idealab-code-challenge.git",
	"scripts": {
		"start": "npm run build && node dist/app.js",
		"build": "npm run clean && babel ./server -d dist",
		"clean": "rm -rf dist",
		"start:dev": "cross-env NODE_ENV=development nodemon ./server/app.js --exec babel-node",
		"pretest": "cross-env NODE_ENV=test npm run undo-migrate && cross-env NODE_ENV=test npm run migrate",
		"test": "jest --no-cache  --detectOpenHandles --runInBand --coverage --colors --forceExit",
		"migrate": "node_modules/.bin/sequelize db:migrate",
		"undo-migrate": "node_modules/.bin/sequelize db:migrate:undo:all",
		"seed": "node_modules/.bin/sequelize db:seed:all --debug",
		"undo-seed": "node_modules/.bin/sequelize db:seed:undo:all",
		"drop-db": "node_modules/.bin/sequelize db:drop",
		"raw": "cross-env NODE_ENV=development babel-node ./server/helpers/query.js"
	},
	"keywords": [
		"nodejs",
		"express"
	],
	"author": "Okwukwe Ewurum",
	"license": "MIT",
	"dependencies": {
		"@hapi/joi": "^16.1.7",
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"fancy-log": "^1.3.3",
		"jsonwebtoken": "^8.5.1",
		"lodash.isempty": "^4.4.0",
		"morgan": "^1.9.1",
		"nodemailer": "^6.4.16",
		"nodemon": "^2.0.0",
		"pg": "^7.14.0",
		"pg-hstore": "^2.3.3",
		"sequelize": "^5.21.2",
		"sequelize-cli": "^5.5.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.3",
		"@babel/core": "^7.4.3",
		"@babel/node": "^7.2.2",
		"@babel/plugin-proposal-object-rest-spread": "^7.6.2",
		"@babel/plugin-transform-runtime": "^7.6.2",
		"@babel/preset-env": "^7.4.3",
		"babel-jest": "^24.7.1",
		"chai": "^4.2.0",
		"cross-env": "^6.0.3",
		"eslint": "^5.3.0",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-config-prettier": "^4.1.0",
		"eslint-plugin-import": "^2.16.0",
		"eslint-plugin-jsx-a11y": "^6.2.1",
		"eslint-plugin-prettier": "^3.0.1",
		"eslint-plugin-react": "^7.12.4",
		"jest": "^24.7.1",
		"mocha": "^6.1.4",
		"prettier": "^1.16.4",
		"supertest": "^4.0.2"
	}
}
